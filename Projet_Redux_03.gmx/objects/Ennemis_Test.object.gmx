        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Déplacement

//Variable
var rkey = keyboard_check(ord("D"));
var lkey = keyboard_check(ord("Q"));
var jkey = keyboard_check(ord("M"));

// Gravity
if (vspd &lt; 10)
    {
    vspd += grav;
    }
    
//check for ground            
if ((place_meeting(x, y+1,Block_Test))) 
{
    
    // jumping
    if (keyboard_check_pressed(ord("M")))
        { 
        vspd = -jspd;
        }
}

// On bubble
if (place_meeting(x, y+1,obj_Bulle_Normal))
{
// jumping
    if (jkey)
        { 
        vspd = -jspd;
        }
}
/*
// Check On bubble
if (place_meeting(x, y+1,obj_Bubble_bud_Nrml))|| (place_meeting(x, y+1,obj_boule_Feu))|| (place_meeting(x, y+1,obj_Boule_Eclair)) || (place_meeting(x, y+1,obj_Boule_Eau))
{
// jumping
    if (jkey)
        { 
        vspd = -jspd;
        }
}
*/

// moving right
if (rkey)
{
    Bub_hspd = spd;
    Direction = 1;
    self.image_xscale = 1;
}

// moving left
if (lkey)
{
    Bub_hspd = -spd;
    Direction = -1;
    self.image_xscale = -1;
}

//Check for not moving
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey))
{
Bub_hspd = 0;
}

//horizontal collisions
if (place_meeting(x+Bub_hspd, y, Block_Test))
{
while (!place_meeting(x+sign(Bub_hspd), y, Block_Test))
    {
        x+= sign(Bub_hspd);
    }
 Bub_hspd = 0;
}
// move horizontally
x += Bub_hspd


//vertical collisions
if (place_meeting(x, y+vspd,Block_Test))
{
while (!place_meeting(x, y+sign(vspd), Block_Test))
  {
    y += sign(vspd);
  }
vspd = 0;
}

// move vertically
y += vspd

//reduce speed on fall
if (sign(vspd) = +1)
{
vspd = 2;
}

/*
// Wrap par le bas
if (place_meeting(x, y+1, obj_platele_02))
    {
    self.y = 0;
    }
*/
/*
// Wrap par le haut
if (place_meeting(x, y+15, obj_platete_01))
    {
    self.y = 200;
    }
*/
</string>
