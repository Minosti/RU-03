var bLaunch = keyboard_check_pressed(ord("L"));

//Tire de bulle
fireTimer -= 1;

if (fireTimer &gt; 0)
{
animShoot -= 1;
}
    
if (fireTimer &lt;= 0)
{
    if (bLaunch)
    { 
    //self.sprite_index=BubShout;
  
        if (Direction = 1)
        {
         BubbleDir = instance_create(x+5, y, obj_Bulle_Tir)
         BubbleDir.hspeed = 10;
         fireTimer = 15;
         //self.sprite_index=BubWalk;
         }
    
        if (Direction = -1)
         {
         BubbleDir = instance_create(x-5, y, obj_Bulle_Tir)
         BubbleDir.hspeed = -10;
         fireTimer = 15;
         //self.sprite_index=BubWalk;
         }
    }
}
