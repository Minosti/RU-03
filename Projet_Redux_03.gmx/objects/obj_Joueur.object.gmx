        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Déplacement

//Variable

var rkey = keyboard_check(ord("D"));
var GP_Dpad_R = gamepad_button_check(0, gp_padr);
var GP_Stick_R = (gamepad_axis_value(0, gp_axislh)&gt;0.8);

var lkey = keyboard_check(ord("Q"));
var GP_Dpad_L = gamepad_button_check(0, gp_padl);
var GP_Stick_L = (gamepad_axis_value(0, gp_axislh)&lt;-0.8);

var jkey = keyboard_check(ord("M"));
var GP_Jump = gamepad_button_check(0, gp_face1);
var GP_Jump_P = gamepad_button_check_pressed(0, gp_face1);

var GP_Crouch = (gamepad_axis_value(0, gp_axislv)&gt;0.9);


//show_debug_message(vspd);

// Gravity
if (vspd &lt; 10) 
    {
    vspd += grav;
    }

if /*(!jkey)*/ (!GP_Jump)&amp;&amp; (vspd &lt; 5) 
{
vspd += grav;
}

//check for ground            
if ((place_meeting(x, y+8,obj_Block_Bub_Collision_V))) 
{

    //Crounch
    if (GP_Crouch) &amp;&amp; (!GP_Jump)
    {
        self.sprite_index=spr_Bub_Crouch;
        OnCrouch = 1
        CrouchEndTimer=0;
    }else{
        OnCrouch = 0
        if (CrouchEndTimer==0)
        {
            if (OnElemental == 0)
            {
            self.sprite_index=spr_Bub_Walk;
            }
            if (OnElemental == 1)
            {
            self.sprite_index=spr_Bub_Walk_Fire;
            }
            if (OnElemental == 2)
            {
            self.sprite_index=spr_Bub_Walk_Water;
            }
            if (OnElemental == 3)
            {
            self.sprite_index=spr_Bub_Walk_Lightning;
            }
        CrouchEndTimer = 1;
        }
    }
    
    // jumping
    if (keyboard_check_pressed(ord("M"))) || (GP_Jump_P)
        { 
        vspd = -jspd;
        }
}

// On bubble
if (place_meeting(x, y+1,obj_Bulle_Normal))|| (place_meeting(x, y+1,obj_Bulle_Feu)) || (place_meeting(x, y+1,obj_Bulle_Eclair))
{
// jumping
    if (jkey) || (GP_Jump)
        { 
        vspd = -jspd;
        }
}

/*
//reduce speed on fall
if (sign(vspd) = +1)
{
vspd = 7;
}
*/

// moving right
if (CooldownDashMode != 1) //Non en Dash
{
if (rkey) || (GP_Dpad_R) || (GP_Stick_R)
{
    Bub_hspd = spd;
    Direction = 1;
    self.image_xscale = 1;
}
}

// moving left
if (CooldownDashMode != 1) //Non en Dash
{
if (lkey) || (GP_Dpad_L) || (GP_Stick_L)
{
    Bub_hspd = -spd;
    Direction = -1;
    self.image_xscale = -1;
}
}
/*
//Check for not moving - PLAY WITH KEYBOARD
if ((!rkey &amp;&amp; !lkey)||(rkey &amp;&amp; lkey))
{
Bub_hspd = 0;
}

//Check for not moving - PLAY WITH PAD
if ((!GP_Dpad_R &amp;&amp; !GP_Dpad_L)||(GP_Dpad_R &amp;&amp; GP_Dpad_L))
{
Bub_hspd = 0;
}
*/

//Check for not moving - PLAY WITH STICK
if ((!GP_Stick_R &amp;&amp; !GP_Stick_L)||(GP_Stick_R &amp;&amp; GP_Stick_L))
{
Bub_hspd = 0;
}

//horizontal collisions
if (place_meeting(x+Bub_hspd, y, obj_Block_Bub_Collision_H))
{
while (!place_meeting(x+sign(Bub_hspd), y, obj_Block_Bub_Collision_H))
    {
        x+= sign(Bub_hspd);
    }
 Bub_hspd = 0;
}
// move horizontally
x += Bub_hspd


//vertical collisions
if (place_meeting(x, y+vspd,obj_Block_Bub_Collision_V))
{
while (!place_meeting(x, y+sign(vspd), obj_Block_Bub_Collision_V))
  {
    y += sign(vspd);
  }
vspd = 0;
}

// move vertically
y += vspd

///////////////////////////////////////////////////////////

if instance_exists(obj_AspirationCollision)
        {
        //Déplacement Aspiration Collision
        AspirationCollision.x = x+(1*Direction);
        AspirationCollision.y = y;
        AspirationCollision.image_xscale = 1*Direction; 
        }

if instance_exists(obj_Eclair_Bouclier)
    {
     Boubou.x = x;
     Boubou.y = y;
    }
