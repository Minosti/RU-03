        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// IA mouvement 

// Check de la position du joueur 
    // VPositionPlayerx Positif : Joueur a GAUCHE du buster 
    // VPositionPlayerx Negatif : Joueur a DROITE du buster 
        vppx = x-obj_Joueur.x;

    // VPositionPlayery Positif : Joueur en HAUT du buster 
    // VPositionPlayery Negatif : Joueur en BAS du buster 
        vppy = y-obj_Joueur.y
    
// choix du deplacement 

if (((vppy &gt; 64) || (vppy &lt; -64)) || ((vppx &gt; 160) || (vppy &lt; -160)))
{
    vcc = 1;
}else
{
    if ( vfuite = 0)
    {
        vtimer--;
        if (vstarttimer = 1)
        {
            vtimer = 390;
        }
        
        if ( vtimer &gt; 0)
        {
            vcc = 2;
            vstarttimer = 0;
            
        }else
        {
            if ( vtimer &gt; -5 )
            {
                vcc = 3;
                vstarttimer = 0;
            
            }else
            {
                vstarttimer = 1
            }
        }
    }
}

vfuite = 0;

if (((vppy &lt; 8) &amp;&amp; (vppy &gt; -8)) &amp;&amp; ((vppx &lt; 64) &amp;&amp; (vppy &gt; -64)))
{
    vcc = 4;
    vfuite = 1;
}

//show_debug_message(vcc);
// Deplacement 

    // VaribaleChangementComportement 1: rapprochement du joueur
if ( vcc = 1 )
{
        if (vppx &lt; vppy)
        {
            if ( vppx &lt; 0)
            {
                rkey = true;
                lkey = false;
                dkey = false;
                jkey = false;
            }else
            {
                rkey = false;
                lkey = true;
                dkey = false;
                jkey = false;
            }
        }else
        {
            if ( vppy &lt; 0)
            {
                rkey = false;
                lkey = false;
                dkey = true;
                jkey = false;
            }else
            {
                rkey = false;
                lkey = false;
                dkey = false;
                jkey = true;
            } 
        }
    if ((vppx &lt; 8) &amp;&amp; (vppx &gt; -8))
    {
        rkey = false;
        lkey = false;
        if (vppy &lt; 0)
        {
            dkey = true;
            jkey = false;
        }else
        {
            dkey = false;
            jkey = true;
        }
    }
    
    if ((vppy &lt; 8) &amp;&amp; (vppy &gt; -8))
    {
        dkey = false;
        jkey = false;
        if (vppx &lt; 0)
        {
            rkey = true;
            lkey = false;
        }else
        {
            rkey = false;
            lkey = true;
        }
    }
}
    
     // VaribaleChangementComportement 2: immobilité de l'incendo et tire
if ( vcc = 2 )
{  
        rkey = false; 
        lkey = false;
        dkey = false;
        jkey = false;
}
 
    // VaribaleChangementComportement 3: mouvement pour trouver une autre position pour tirer
if ( vcc = 3 )
{       
    if ((vppy &lt; 0))
    {
        rkey = false; 
        lkey = false;
        dkey = true;
        jkey = false;
    }else
    {
        rkey = false; 
        lkey = false;
        dkey = false;
        jkey = true;
    }
}


    // VaribaleChangementComportement 4: fuite devant le joueur
if ( vcc = 4)
{ 
    if (vppx &lt; vppy)
    {
        if ( vppy &lt; 0)
        {
            rkey = false;
            lkey = false;
            dkey = false;
            jkey = true;
        }else
        {
            rkey = false;
            lkey = false;
            dkey = true;
            jkey = false;
        }
           
    }else
    {
        if ( vppx &lt; 0)
        {
            rkey = false;
            lkey = true;
            dkey = false;
            jkey = false;
        }else
        {
            rkey = true;
            lkey = false;
            dkey = false;
            jkey = false;
        }
        
    }
}

vtimerdkey--;
vdkey--;
if ((dkey = true) &amp;&amp; (vtimerdkey &lt;= 0))
{
    dkey = true;
    vtimerdkey = 40
    vdkey = 6;
}else 
{
   dkey = false;
}

if (vdkey &gt; 0)
{
    dkey = true;
}  
if ((dkey = true) &amp;&amp; (y &gt;= 200 ))
{
    dkey = false;
}



vtimerjkey--;
vjkey--;
if ((jkey = true) &amp;&amp; (vtimerjkey &lt;= 0))
{
    jkey = true;
    vtimerjkey = 35
    vjkey = 6;
}else 
{
   jkey = false;
}

if (vjkey &gt; 0)
{
    jkey = true;
}  
